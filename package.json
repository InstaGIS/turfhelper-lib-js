{
    "name": "turfhelper-lib-js",
    "version": "1.0.1",
    "description": "A bridge to use Turf along with google Maps API",
    "scripts": {
        "test": "grunt karma"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/InstaGIS/turfhelper-lib-js.git"
    },
    "keywords": [
        "turf",
        "google",
        "maps",
        "wicket"
    ],
    "author": "amenadiel@gmail.com",
    "license": "propietary",
    "bugs": {
        "url": "https://github.com/InstaGIS/turfhelper-lib-js/issues"
    },
    "homepage": "https://github.com/InstaGIS/turfhelper-lib-js#readme",
    "main": "dist/index.js",
    "private": true,
    "dependencies": {
        "grunt": "^1",
        "grunt-karma": "^2",
        "karma": "^1",
        "karma-phantomjs-launcher": "^1",
        "karma-qunit": "^1",
        "jspm": "^0.17.0-beta.25",
        "phantomjs-prebuilt": "^2",
        "qunitjs": "^1"
    },
    "devDependencies": {
        "babel-eslint": "^6",
        "eslint": "^3",
        "lodash": "^4"
    },
    "jspm": {
        "name": "turfhelper-lib-js",
        "main": "dist/index.js",
        "dependencies": {},
        "devDependencies": {
            "assert": "github:jspm/nodelibs-assert@^0.2.0-alpha",
            "buffer": "github:jspm/nodelibs-buffer@^0.2.0-alpha",
            "events": "github:jspm/nodelibs-events@^0.2.0-alpha",
            "fs": "github:jspm/nodelibs-fs@^0.2.0-alpha",
            "path": "github:jspm/nodelibs-path@^0.2.0-alpha",
            "stream": "github:jspm/nodelibs-stream@^0.2.0-alpha",
            "turf-along": "npm:turf-along@^1.0.2",
            "turf-buffer": "npm:turf-buffer@^1.1.0",
            "turf-centroid": "npm:turf-centroid@^1.1.3",
            "turf-featurecollection": "npm:turf-featurecollection@^1.0.1",
            "turf-inside": "npm:turf-inside@^1.1.4",
            "turf-line-slice": "npm:turf-line-slice@^1.3.4",
            "turf-linestring": "npm:turf-linestring@^1.0.2",
            "turf-merge": "npm:turf-merge@^1.0.2",
            "turf-point": "npm:turf-point@^2.0.1",
            "turf-polygon": "npm:turf-polygon@^1.0.3",
            "turf-simplify": "npm:turf-simplify@^1.0.3",
            "turf-union": "npm:turf-union@^1.0.2",
            "underscore": "npm:underscore@^1.8.3",
            "util": "github:jspm/nodelibs-util@^0.2.0-alpha",
            "vm": "github:jspm/nodelibs-vm@^0.2.0-alpha"
        },
        "peerDependencies": {
            "plugin-babel": "npm:systemjs-plugin-babel@^0.0.13",
            "gmap": "npm:amd-googlemaps-loader@^1.6",
            "process": "github:jspm/nodelibs-process@^0.2.0-alpha"
        },
        "overrides": {
            "npm:inherits@2.0.1": {
                "ignore": [
                    "test.js"
                ]
            },
            "npm:readable-stream@1.1.14": {
                "map": {
                    "stream": "stream-browserify/index"
                },
                "systemjs": {
                    "main": "readable.js"
                },
                "dependencies": {
                    "core-util-is": "~1.0.1",
                    "inherits": "~2.0.1",
                    "isarray": "0.0.1",
                    "string_decoder": "~0.10.0",
                    "stream-browserify": "^1.0.1"
                }
            }
        }
    }
}
