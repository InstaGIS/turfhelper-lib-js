        "grunt-karma": "^2",
        "karma": "^1",
        "karma-phantomjs-launcher": "^1",
        "karma-qunit": "^1",
        "jspm": "^0.17.0-beta.25",
        "phantomjs-prebuilt": "^2",
        "qunitjs": "^1"
    },
    "devDependencies": {
        "babel-eslint": "^6",
        "eslint": "^3",
        "lodash": "^4"
