{
    "name": "turfhelper-lib-js",
    "version": "1.1.3",
    "description": "A bridge to use Turf along with google Maps API",
    "scripts": {
        "test": "grunt karma"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/InstaGIS/turfhelper-lib-js.git"
    },
    "keywords": [
        "turf",
        "google",
        "maps",
        "wicket"
